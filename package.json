{
  "name": "react-modal-syaku",
  "version": "1.0.3",
  "description": "react modal",
  "author": "Syaku (최석균) <http://syaku.tistory.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "react-modal",
    "modal",
    "js",
    "es6"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/syakuis/react-modal"
  },
  "main": "dist/react-modal.js",
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.js?(x)"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
      "NODE_ENV": "test"
    }
  },
  "config": {
    "filename": "react-modal",
    "publicPath": "/",
    "output": "dist",
    "src": "src",
    "entry": "./src/index.js",
    "port": 8088
  },
  "scripts": {
    "test": "jest",
    "build:dev": "webpack --devtool source-map --colors --progress -d",
    "build:prod": "webpack --colors --progress -p",
    "build:demo": "webpack --config webpack.config.demo.js --devtool source-map --colors --progress -d",
    "serv:demo": "webpack-dev-server --config webpack.config.demo.js --devtool source-map --colors --progress -d",
    "serv:demo2": "webpack-dev-server --config webpack.config.demo2.js --devtool source-map --colors --progress -d",
    "serv:dev": "webpack-dev-server --devtool source-map --colors --progress -d",
    "serv:prod": "webpack-dev-server --colors --progress -p --compress",
    "state:dev": "webpack --config webpack.state.config.js --colors --progress -d --profile --json > stats-dev.json",
    "state:prod": "webpack --config webpack.state.config.js --colors --progress -p --profile --json > stats.json"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "bundle-loader": "^0.5.5",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^21.1.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.2",
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^1.0.2",
    "grunt-contrib-uglify": "^2.2.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^21.1.0",
    "lodash": "^4.17.4",
    "react-addons-perf": "^15.4.2",
    "style-loader": "^0.18.2",
    "stylelint": "7.12.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "url-loader": "^0.5.9",
    "webpack": "2.6.1",
    "webpack-bundle-analyzer": "^2.3.0",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^3.3.7",
    "flow-bin": "0.54.1",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.1",
    "prop-types": "^15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-modal-syaku": "^1.0.2",
    "shortid": "^2.2.8",
    "styled-components": "^2.1.2"
  }
}

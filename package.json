{
  "name": "react-modal-syaku",
  "version": "1.1.1",
  "description": "react modal",
  "author": "Syaku (http://syaku.tistory.com)",
  "license": "MIT",
  "keywords": [
    "syaku",
    "react",
    "react-modal",
    "modal",
    "js",
    "es6"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/syakuis/react-modal"
  },
  "main": "dist/react-modal.js",
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.js?(x)"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
      "NODE_ENV": "test"
    }
  },
  "config": {
    "filename": "react-modal",
    "publicPath": "/",
    "dist": "dist",
    "src": "src",
    "entry": "./src/index.js",
    "port": 8088,
    "externals": {
      "react": {
        "root": "React",
        "commonjs2": "react",
        "commonjs": "react",
        "amd": "react"
      },
      "react-dom": {
        "root": "ReactDOM",
        "commonjs2": "react-dom",
        "commonjs": "react-dom",
        "amd": "react-dom"
      }
    }
  },
  "scripts": {
    "test": "jest",
    "build:prod": "webpack --colors --progress",
    "serv:dev": "webpack-dev-server --config webpack.config.dev.js --devtool source-map --colors --progress -d",
    "serv:demo": "webpack-dev-server --config webpack.config.demo.js --devtool source-map --colors --progress -d",
    "state:dev": "webpack --config webpack.state.config.js --colors --progress -d --profile --json > stats-dev.json",
    "state:prod": "webpack --config webpack.state.config.js --colors --progress -p --profile --json > stats.json"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.22.0",
    "bootstrap": "^3.3.7",
    "bundle-loader": "^0.5.5",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.2",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^2.28.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "style-loader": "^0.18.2",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "url-loader": "^0.5.9",
    "webpack": "2.6.1",
    "webpack-bundle-analyzer": "^2.3.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "core-js": "^2.5.1",
    "prop-types": "^15.5.10",
    "shortid": "^2.2.8"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  }
}

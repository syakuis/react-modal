{
  "name": "react-modal-syaku",
  "version": "1.2.3",
  "description": "react modal",
  "author": "Syaku (http://syaku.tistory.com)",
  "license": "MIT",
  "keywords": [
    "syaku",
    "react",
    "react-modal",
    "modal",
    "js",
    "es6"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/syakuis/react-modal"
  },
  "main": "dist/react-modal.js",
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.js?(x)"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
      "NODE_ENV": "test"
    }
  },
  "config": {
    "filename": "react-modal",
    "publicPath": "",
    "dist": "dist",
    "src": "src",
    "entry": "./src/index.js",
    "port": 8088,
    "externals": {
      "react": {
        "root": "React",
        "commonjs2": "react",
        "commonjs": "react",
        "amd": "react"
      },
      "react-dom": {
        "root": "ReactDOM",
        "commonjs2": "react-dom",
        "commonjs": "react-dom",
        "amd": "react-dom"
      }
    }
  },
  "scripts": {
    "jest": "jest",
    "p:remove": "yarn remove react-modal-syaku -D",
    "build:prod": "NODE_ENV='production' webpack -p --profile --colors --progress",
    "build:demo": "NODE_ENV='production' SOURCE_TARGET='node' webpack --config webpack.config.demo.js -p --profile --colors --progress",
    "serv:dev": "NODE_ENV='development' SOURCE_TARGET='local' webpack-dev-server -d --config webpack.config.dev.js --colors --progress --profile",
    "serv:demo": "NODE_ENV='development' SOURCE_TARGET='node' webpack-dev-server -d --config webpack.config.demo.js --colors --progress --profile",
    "stats:prod": "NODE_ENV='production' webpack --config webpack.config.stats.js -p --colors --progress --profile --json > stats.json"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.22.0",
    "bootstrap": "^3.3.7",
    "bundle-loader": "^0.5.5",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^2.28.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "react-modal-syaku": "^1.2.3",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "core-js": "^2.5.1",
    "prop-types": "^15.6.0",
    "shortid": "^2.2.8"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  }
}
